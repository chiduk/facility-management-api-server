{
  "name": "fm-api-server",
  "version": "3.0.5",
  "description": "An api server for facility management",
  "main": "dist/index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.0.0 <19"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:build": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"yarn compile:watch\" \"yarn pre:dev\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest -i --colors --verbose --detectOpenHandles --logHeapUsage",
    "test:ts": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest -i --watch --colors --verbose --logHeapUsage --detectOpenHandles -- 'ts$'",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles -- 'js$'",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest -i --watchAll",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage",
    "coverage:coveralls": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage --coverageReporters=lcov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky install",
    "release": "standard-version && git push --follow-tags origin master",
    "commit": "git add -A && cz"
  },
  "keywords": [
    "node",
    "typescript",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.4.0",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.13",
    "@types/express": "^4.17.16",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.2.6",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.11",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "concurrently": "^7.1.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "lint-staged": "^13.1.0",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.271.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^11.7.0",
    "helmet": "^6.0.1",
    "http-status": "^1.5.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.4.6",
    "mongoose-autopopulate": "^0.17.1",
    "mongoose-paginate-v2": "^1.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^5.1.2",
    "redis": "^4.6.3",
    "swagger-cli": "^4.0.4",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.6.2",
    "validator": "^13.7.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
